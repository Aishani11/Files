#include<stdio.h>
#include<conio.h>
int main()

{
    int i,j,k=0;
    for(i=0;i<=5;i++)
    {
        for(j=0;j<=i;j++)
        {
            if(k%2==1)
            {
                printf("01");
            }
            else
            {
                printf("01");
            }
            k++;
        }
        printf("\n");
    }
    getch();
}
